{
  "name": "react-native-css-modules-theme",
  "version": "1.0.0",
  "description": "Theme support for CSS modules in React Native environment",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "test": "jest"
  },
  "keywords": [
    "css",
    "modules",
    "theme",
    "react",
    "native"
  ],
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "23.1.1",
    "@types/react-native": "0.55.22",
    "jest": "23.1.0",
    "react-native": "0.55.4",
    "ts-jest": "22.4.6",
    "typescript": "2.9.2"
  },
  "peerDependencies": {
    "react-native": "^0.50.x"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
